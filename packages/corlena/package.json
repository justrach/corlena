{
  "name": "corlena",
  "version": "1.3.0",
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src",
    "react",
    "svelte",
    "wasm"
  ],
  "scripts": {
    "build": "node ./scripts/sync-wasm.mjs"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "svelte": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./wasm": {
      "types": "./wasm/index.d.ts",
      "default": "./wasm/index.js"
    },
    "./react": {
      "types": "./react/index.d.ts",
      "default": "./react/index.js"
    },
    "./svelte": {
      "svelte": "./svelte/index.js",
      "default": "./svelte/index.js"
    }
  },
  "dependencies": {
    "@corlena/wasm": "^0.0.3"
  },
  "peerDependencies": {
    "svelte": "^4 || ^5",
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "svelte": { "optional": true },
    "react": { "optional": true }
  },
  "devDependencies": {
    "svelte": "^4.2.18",
    "typescript": "^5.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
